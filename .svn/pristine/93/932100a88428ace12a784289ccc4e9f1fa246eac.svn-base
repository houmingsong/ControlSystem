package com.j1635web.bean;

import java.io.Serializable;

/**
 * 小班实体bean
 * @author 乔荣升
 *
 */
public class ClassBean implements Serializable{
	/**
	 * 序列化id
	 */
	private static final long serialVersionUID = 1L;
	/**
	 * 小班id
	 */
	private Integer id;
	/**
	 *小班名称
	 */
	private String className;
	/**
	 *负责人
	 */
	private String principal;
	/**
	 *负责人电话
	 */
	private String tel;
	/**
	 *人员数量
	 */
	private Integer personNumber;
	/**
	 *负责区域id
	 */
	private Integer areaId;
	/**
	 *负责区域名称
	 */
	private String areaName;
	/**
	 * 区域实体
	 */
	private AreaBean areaBean;
	public ClassBean() {
		super();
		// TODO Auto-generated constructor stub
	}

	@Override
	public String toString() {
		return "ClassBean [id=" + id + ", className=" + className + ", principal=" + principal + ", tel=" + tel
				+ ", personNumber=" + personNumber + ", areaId=" + areaId + ", areaName=" + areaName + ", areaBean="
				+ areaBean + "]";
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getClassName() {
		return className;
	}
	public void setClassName(String className) {
		this.className = className;
	}
	public String getPrincipal() {
		return principal;
	}
	public void setPrincipal(String principal) {
		this.principal = principal;
	}
	public String getTel() {
		return tel;
	}
	public void setTel(String tel) {
		this.tel = tel;
	}
	public Integer getPersonNumber() {
		return personNumber;
	}
	public void setPersonNumber(Integer personNumber) {
		this.personNumber = personNumber;
	}
	public Integer getAreaId() {
		return areaId;
	}
	public void setAreaId(Integer areaId) {
		this.areaId = areaId;
	}
	public String getAreaName() {
		return areaName;
	}
	public void setAreaName(String areaName) {
		this.areaName = areaName;
	}
	public AreaBean getAreaBean() {
		return areaBean;
	}
	public void setAreaBean(AreaBean areaBean) {
		this.areaBean = areaBean;
	}
	public ClassBean(String className, String principal, String tel, Integer personNumber, Integer areaId) {
		super();
		this.className = className;
		this.principal = principal;
		this.tel = tel;
		this.personNumber = personNumber;
		this.areaId = areaId;
	}

}
